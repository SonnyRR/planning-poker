{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.BrowserConsole",
      "Serilog.Sinks.BrowserHttp",
      "PlanningPoker.SharedKernel"
    ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "BrowserConsole",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {CorrelationId} {Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "BrowserHttp"
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithCorellationId",
      "WithInstanceId"
    ]
  },
  "ProSettings": {
    "NavTheme": "dark",
    "Layout": "side",
    "ContentWidth": "Fluid",
    "FixedHeader": false,
    "FixSiderbar": true,
    "Title": "Ant Design Pro",
    "PrimaryColor": "daybreak",
    "ColorWeak": false,
    "SplitMenus": false,
    "HeaderRender": true,
    "FooterRender": true,
    "MenuRender": true,
    "MenuHeaderRender": true,
    "HeaderHeight": 48
  }
}