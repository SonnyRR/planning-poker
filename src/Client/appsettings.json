{
  "Logging": {
    "Using": [
      "BrowserConsole",
      "BrowserHttp",
      "PlanningPoker.SharedKernel.Extensions"
    ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "BrowserConsole"
      },
      {
        "Name": "BrowserHttp"
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithCorellationId",
      "WithInstanceId"
    ]
  }
}