{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.BrowserConsole",
      "Serilog.Sinks.BrowserHttp",
      "PlanningPoker.SharedKernel"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "BrowserConsole"
      },
      {
        "Name": "BrowserHttp"
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithInstanceId"
    ]
  },
  "AntSettings": {
    "NavTheme": "dark",
    "Layout": "side",
    "ContentWidth": "Fluid",
    "FixedHeader": false,
    "FixSiderbar": true,
    "Title": "Planning Poker",
    "PrimaryColor": "daybreak",
    "ColorWeak": false,
    "SplitMenus": false,
    "HeaderRender": true,
    "FooterRender": true,
    "MenuRender": true,
    "MenuHeaderRender": true,
    "HeaderHeight": 48
  },
  "LayoutSettings": {
    "Author": "Vasil Kotsev",
    "Links": [
      {
        "Key": "github",
        "Title": "GitHub",
        "Href": "https://github.com/SonnyRR",
        "BlankTarget": true
      }
    ]
  }
}